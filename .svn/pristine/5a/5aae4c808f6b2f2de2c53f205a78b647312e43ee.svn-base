package com.digitexx.ancestry.bean;

import java.io.Serializable;

/**
 * @author lqnhu
 * 
 */
@SuppressWarnings("serial")
public class BeanDefineRule implements Serializable{
	private long id;
	private String rule_title;
	private String rule_name;
	private String rule_value;
	private String note;
	private int status;

	/**
	 * @return the id
	 */
	public long getId() {
		return id;
	}

	/**
	 * @return the rule_title
	 */
	public String getRule_title() {
		return rule_title;
	}

	/**
	 * @return the rule_name
	 */
	public String getRule_name() {
		return rule_name;
	}

	/**
	 * @return the rule_value
	 */
	public String getRule_value() {
		return rule_value;
	}

	/**
	 * @return the note
	 */
	public String getNote() {
		return note;
	}

	/**
	 * @return the status
	 */
	public int getStatus() {
		return status;
	}

	/**
	 * @param id
	 *            the id to set
	 */
	public void setId(long id) {
		this.id = id;
	}

	/**
	 * @param rule_title
	 *            the rule_title to set
	 */
	public void setRule_title(String rule_title) {
		this.rule_title = rule_title;
	}

	/**
	 * @param rule_name
	 *            the rule_name to set
	 */
	public void setRule_name(String rule_name) {
		this.rule_name = rule_name;
	}

	/**
	 * @param rule_value
	 *            the rule_value to set
	 */
	public void setRule_value(String rule_value) {
		this.rule_value = rule_value;
	}

	/**
	 * @param note
	 *            the note to set
	 */
	public void setNote(String note) {
		this.note = note;
	}

	/**
	 * @param status
	 *            the status to set
	 */
	public void setStatus(int status) {
		this.status = status;
	}
	
	/* (non-Javadoc)
	 * @see java.lang.Object#toString()
	 */
	@Override
	public String toString() {
		return rule_name;
	}

}
